<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.tickler">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/food"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/heatbg1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".heatCal"
            android:label="@string/title_activity_heat_cal"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Index"
            android:label="@string/title_activity_index"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".milk"
            android:label="@string/title_activity_milk"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".fruit"
            android:label="@string/title_activity_fruit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".vgetable"
            android:label="@string/title_activity_vgetable"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".staple"
            android:label="@string/title_activity_staple"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".history"
            android:label="@string/title_activity_staple"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>